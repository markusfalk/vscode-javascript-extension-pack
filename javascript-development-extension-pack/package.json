{
  "name": "javascript-development-extension-pack",
  "displayName": "JavaScript Development Extension Pack",
  "description": "This exension pack is a currated list of all the extension you may need for JavaScript development.",
  "version": "1.2.0",
  "scripts": {
    "vsce": "vsce",
    "vsce:package": "vsce package",
    "vsce:publish": "vsce publish",
    "vscode:publish": "npm run vsce:publish",
    "postversion": "git push && git push --tags && npm run vsce:package"
  },
  "engines": {
    "vscode": "^1.36.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/markusfalk/vscode-javascript-extension-pack"
  },
  "publisher": "MarkusFalk",
  "icon": "logo-javascript.png",
  "categories": [
    "Extension Packs"
  ],
  "extensionPack": [
    "coenraads.bracket-pair-colorizer",
    "dbaeumer.vscode-eslint",
    "editorconfig.editorconfig",
    "eg2.vscode-npm-script",
    "esbenp.prettier-vscode",
    "formulahendry.auto-rename",
    "hookyqr.beautify",
    "joelday.docthis",
    "johnpapa.vscode-peacock",
    "juliensanmartin.vscode-depcheck",
    "kisstkondoros.vscode-codemetrics",
    "mkaufman.htmlhint",
    "msjsdiag.debugger-for-chrome",
    "naumovs.color-highlight",
    "pranaygp.vscode-css-peek",
    "wayou.vscode-todo-highlight",
    "zignd.html-css-class-completion"
  ]
}
